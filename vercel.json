{
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "ebnalarab.com" }],
      "destination": "https://www.ebnalarab.com/:path*",
      "statusCode": 301
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
